<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="louisefirstmodule_objet" resource="default" engine="innodb" comment="Comment">
        <column xsi:type="smallint" name="objet_id" padding="6" unsigned="false" nullable="false" identity="true"
                comment="Objet ID"/>
        <column xsi:type="varchar" name="title" nullable="true" length="255" />
        <column xsi:type="varchar" name="content" nullable="true" length="255" />
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="objet_id"/>
        </constraint>
    </table>
</schema>