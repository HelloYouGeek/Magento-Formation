<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/louisemodule/getuppertextofstring" method="GET">
        <service class="Louise\FirstModule\Api\ObjetRepositoryInterface" method="getUpperTextOfString"/>
        <resources>
            <resource ref="Louise_FirstModule::general"/>
        </resources>
    </route>
    <route url="/V1/louisemodule/getobjetbyid" method="GET">
        <service class="Louise\FirstModule\Api\ObjetRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>